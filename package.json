{
  "name": "vue-view-json",
  "version": "0.1.5",
  "description": "A Vue component for rendering JSON data as a tree structure.",
  "author": "guochengfeng <1065102410@qq.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:guochengfeng/vue-view-json.git"
  },
  "homepage": "https://guochengfeng.github.io/vue-view-json",
  "keywords": [
    "vue",
    "json",
    "format",
    "json-view",
    "vue-component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guochengfeng/vue-view-json/issues"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/vue-view-json.umd.cjs",
  "module": "./dist/vue-view-json.js",
  "exports": {
    ".": {
      "import": "./dist/vue-view-json.umd.cjs",
      "require": "./dist/vue-view-json.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
